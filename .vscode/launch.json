{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Debug AUTH_SERVICE",
      "remoteRoot": "/usr/src/app/",
      "localRoot": "${workspaceFolder}/auth/",
      "protocol": "inspector",
      "port": 9229,
      "restart": true,
      "address": "localhost",
      "sourceMaps": true,
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}